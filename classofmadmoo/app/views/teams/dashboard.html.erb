
<table>
	<thead>
		<th><h1>Project</h1></th>
	</thead>
	<tbody>
		<% @projects.all.each do |project| %>
			<tr><td><%=link_to project.name , project_url(project)%></td></tr>

		<% end %>
	</tbody>
<table>
<table>
	<thead>
		<th><h1>Forums</h1></th>
	</thead>
	<tbody>
		<% @forums.all.each do |forum| %>
			<tr><td><%=link_to forum.name , forum_url(forum)%></td></tr>

		<% end %>
	</tbody>
<table>

<h2>Students</h2>
<%@students.each do |student|%>
<h3><%=student.email%></h3>
<%end%>